<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.calvin.cs.kimprototypeapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <uses-permission android:name="android.permission.INTERNET"/>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name=".DetailActivity"
            android:label="@string/title_activity_detail" >
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="SettingsActivity"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.calvin.cs.kimprototypeapp.MainActivity" />
        </activity>
        <activity
            android:name=".PortfolioActivity"
            android:label="@string/title_activity_portfolio" >
        </activity>
        <activity
            android:name=".TrainingActivity"
            android:label="@string/title_activity_training" >
        </activity>
        <activity
            android:name=".StockPitchActivity"
            android:label="@string/title_activity_stock_pitch" >
        </activity>
        <activity
            android:name=".AboutActivity"
            android:label="@string/title_activity_about" >
        </activity>
        <activity
            android:name=".LogoutActivity"
            android:label="@string/title_activity_logout" >
        </activity>
    </application>

</manifest>
